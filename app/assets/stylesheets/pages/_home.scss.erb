.user--section {
  margin: 0 auto;
  padding-top: 10vh;
}

.showcase {
  img {
    width: 80%;
  }
}

.web {
  header {
    @include text-shadow;
    background-size: cover;
    background-position: top;
  }

}

@media (min-width: 1024px) {
  .web {
    header {
      background-position: center;
    }
  }
}
